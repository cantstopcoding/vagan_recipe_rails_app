<h1>Create New Recipe</h1>
<%= form_for(@recipe) do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <%= f.label :ingredients %>
  <br>
  <%= f.text_field :ingredients %>
  <br>
  <%= f.label :instructions %>
  <br>
  <%= f.text_area :instructions %>
  <br>
  <%= f.label :image_url %>
  <br>
  <%= f.text_field :image_url %>
  <br>
  <%= f.submit %>
<% end %>
